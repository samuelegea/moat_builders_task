<p id="notice"><%= notice %></p>
<div class='card p-4'>
  <p>
    <strong>Artist:</strong>
    <%= @album.artist %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= @album.name %>
  </p>

  <p>
    <strong>Year:</strong>
    <%= @album.year %>
